@article{uzoukwu_using_2010-1,
	title = {Using {GIS} to {Detect} {Changes} in {Land} {Use} {Land} {Cover} for {Electrical} {Transmission} {Line} {Sitting} and {Expansion} {Planning} in {Winona} {County}, {Minnesota}, {USA}},
