	keywords = {ANIMALS-HABITAT-FRAGMENTATION, BIODIVERSITY, BIODIVERSITY-FUNCTIONAL DIVERSITY, BIODIVERSITY-SPECIES DIVERSITY, CONSERVATION, ECOSYSTEM-MANAGEMENT, LANDSCAPE-ECOLOGY},
