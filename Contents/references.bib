	keywords = {Biodiversity conservation, IUCN Red List of Ecosystems, Ecosystem risk assessment, Hierarchical assessment, Southwestern China},
	pages = {152--161},
	file = {Attachment:C\:\\Users\\MatlalaM\\Zotero\\storage\\8MBZF74T\\Tan et al. - 2017 - Preliminary assessment of ecosystem risk based on IUCN criteria in a hierarchy of spatial domains A case study in So.pdf:application/pdf},
