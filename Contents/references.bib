	doi = {10.17159/sajs.2016/20150359},
	note = {ISSN: 19967489
Publication Title: South African Journal of Science},
	keywords = {Biodiversity, Natural resource management, Ecosystem services, Earth stewardship, Eco-labels},
	file = {Attachment:C\:\\Users\\MatlalaM\\Zotero\\storage\\JXF47IJ7\\Barendse, Currie, Wilson - 2016 - Stewardship in South Africa A broader view of stewardship to achieve conservation and sustainability g.pdf:application/pdf},
