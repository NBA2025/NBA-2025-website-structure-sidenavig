	year = {2018},
	note = {ISBN: 1111111111},
	pages = {1--16},
	file = {Attachment:C\:\\Users\\MatlalaM\\Zotero\\storage\\GNBXB92X\\Brauneder et al. - 2018 - Global screening for critical habitat in the terrestrial realm.pdf:application/pdf},
}

