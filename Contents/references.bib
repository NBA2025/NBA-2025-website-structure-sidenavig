@article{affairs_eolfna_2013,
	title = {{EOlf}∼∼{NA}},
	issn = {2048-8726},
	doi = {10.1177/2048872612461726 LK - http://sfx.library.uu.nl/utrecht?sid=EMBASE&issn=20488726&id=doi:10.1177%2F2048872612461726&atitle=High+level+of+multifocal+atherosclerosis+in+patients+with+coronary+artery+disease%3A+Analysis+of+association+with+polymorphism+of+several+candidate+genes&stitle=Eur.+Heart+J.+Acute+Cardiovasc.+Care&title=European+Heart+Journal%3A+Acute+Cardiovascular+Care&volume=1&issue=&spage=78&epage=&aulast=Postadzhiyan&aufirst=A.&auinit=A.&aufull=Postadzhiyan+A.&coden=&isbn=&pages},
	abstract = {Purpose: Atherosclerosis is a multifactorial disease often affecting different territories - coronary, cerebral and arteries of lower extremities. The objective of this study was to evaluate if a polymorphism of several candidate genes were associated with the presence of carotid and peripheral atherosclerosis in 200 patients with angiographically proven coronary artery disease. Methods: Several duplex ultrasonography (DUS) measures were used to assess the atherosclerotic process - intima-media thickening (IMT) or presence of plaques in common carotid (CCA) and femoral artery (CFA), screening for abdominal aneurysm and ankle-brachial index. PCR based genotyping was applied for angiotensin converting enzyme (ACE I/D), eNOS G894T, endothelin -1 138A I/D and matrix mettaloproteinase 3 5A/6A. In all patients (125 pts with ACS and 75 pts with stable angina pectoris) the presence and localization of coronary atherosclerosis was documented by QCA. Results: The prevalence of multifocal atherosclerosis defined by presence of atherosclerotic plaques of carotid and peripheral arteries in DUS examination was 29.5\%. The carotid and femoral IMT did not differ in relation to variations of the studied genotype (p = 0, 34 for ACE, 0, 89 for eNOS, 0, 12 for ET-1 and 0, 74 MMP3 polymorphisms). Multofocal atherosclerosis assessed by the presence of plaques was not associated with the eNOS G684T polymorphism (p = 0, 18). In opposite, the odds of MFA were 2, 57 (1, 3-4, 8, p = 0, 005) in ACE DD carriers, 1, 22 (0, 6-2, 3, p = 0, 05) in MMP3 5A5A and 2, 27 (1, 3-3, 9, p = 0, 03) in ET1 138 II carriers. In a multivariate logistic regression analysis including cardiovascular risk factors and SNPs, the combination of 2 variables ACE DD and ET-1 138 II remain significantly associated with the presence of MFA - OR 4, 47 (1, 1-7, 3, p = 0, 03). Conclusions: Our study demonstrates a high level of multifocal atherosclerosis in patients with coronary artery disease and found associations of MFA with DD genotype of ACE and II genotype of ET-1.},
	number = {36432},
	author = {Affairs, Department of Environmental},
	year = {2013},
	pages = {6--20},
}

